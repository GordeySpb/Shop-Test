{
  "extends": "airbnb",

  "env": {
    "node": true,
    "browser": true
  },

  "plugins": [
    "jsx-a11y"
  ],


  
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-shadow": [2, {"builtinGlobals": false, "hoist": "never", "allow": []}],
    "parser": "babel-eslint",
    "prefer-const": ["error", {
      "destructuring": "any",
      "ignoreReadBeforeAssign": true
    }]
  }
}